{
    "compilerOptions": {
        "target": "es5",
        "lib": [
            "es6",
            "dom"
        ],
        "module": "commonjs",
        "moduleResolution": "node",
        "removeComments": false,
        "sourceMap": true,
        "declaration": true,
        "noImplicitAny": true,
        "noUnusedParameters": false,
        "noUnusedLocals": true,
        "skipLibCheck": true,
        "strict": true,
        "alwaysStrict": true
    }
}
